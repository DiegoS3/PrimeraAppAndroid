<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_addnote" android:id="@+id/mainIbtnAddNota"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="48dp" app:layout_constraintEnd_toStartOf="@+id/mainIbtnSendMail"
            android:layout_marginEnd="35dp"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_sendmail"
            android:id="@+id/mainIbtnSendMail"
            android:layout_marginEnd="48dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp" android:layout_marginStart="35dp"
            app:layout_constraintStart_toEndOf="@+id/mainIbtnAddNota"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_addcita" android:id="@+id/mainIbtnAddCita"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/mainIbtnAddNota" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="48dp" app:layout_constraintEnd_toStartOf="@+id/mainIbtnAddFriend"
            android:layout_marginEnd="35dp"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_addfriend"
            android:id="@+id/mainIbtnAddFriend"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/mainIbtnSendMail" android:layout_marginEnd="48dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="35dp"
            app:layout_constraintStart_toEndOf="@+id/mainIbtnAddCita"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_about" android:id="@+id/mainIbtnAbout"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="375dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginBottom="90dp" android:layout_marginEnd="139dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="139dp" app:layout_constraintHorizontal_bias="0.444"/>
</androidx.constraintlayout.widget.ConstraintLayout>